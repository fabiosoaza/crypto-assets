<?xml version="1.0" encoding="utf-8"?>
<resources>




    <style name="scrollTotalCrypto">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="defaultFabButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:backgroundTint">@color/colorPrimary</item>
    </style>

    <style name="fabContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">12dp</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>


    <style name="bottomBarContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">bottom|end</item>
    </style>

    <style name="bottomNavigationMenu" parent="@style/Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>



</resources>